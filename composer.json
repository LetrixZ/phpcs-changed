{
    "name": "sirbrillig/phpcs-diff",
    "description": "Library to filter phpcs reports for new changes only",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Payton Swick",
            "email": "payton@foolord.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/phpunit --color=always --verbose"
    },
    "autoload": {
        "psr-4": {
            "PhpcsDiff\\": "PhpcsDiff/"
        }
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.2.1",
        "limedeck/phpunit-detailed-printer": "^3.1",
        "sirbrillig/phpcs-variable-analysis": "^2.1.3",
        "sirbrillig/phpcs-import-detection": "^1.1.1"
    }
}
